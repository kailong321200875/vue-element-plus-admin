    "qs": "^6.12.3",
    "url": "^0.11.3",
    "vue": "3.4.32",
    "vue-draggable-plus": "^0.5.2",
    "vue-i18n": "9.13.1",
    "vue-json-pretty": "^2.4.0",
    "vue-router": "^4.4.0",
    "vue-types": "^5.1.3",
    "xgplayer": "^3.0.18"
